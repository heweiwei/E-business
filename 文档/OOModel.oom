	Document document = this.readXML();
	StringBuffer path = new StringBuffer(&quot;/users/user[1=1&quot;);
	if (admin != null) {
		if (admin.getUsername() != null
				&amp;&amp; !admin.getUsername().equals(&quot;&quot;)) {
			path.append(&quot; and contains(username,&#39;&quot;).append(
					admin.getUsername()).append(&quot;&#39;)&quot;);
		}
